.btn-primary {
	border-top: 1px solid;
	border-bottom: 1px solid;
	border-left:none;
	border-right: 1px solid;
	position:relative;
	display:inline-block;
	overflow:visible;
	margin:40px 35px;
	padding: 7px 70px;
		max-width:100%;
	word-wrap:normal;
	white-space:normal;
	text-transform:none;
	font-family: "Oswald";
	font-size:16px;

	
	
}

.btn-primary::before {
		content:url("/wp-content/uploads/2020/04/logo-brand-left.png");
	position:absolute;
	top:-18px;
	left:-40px;

	
}

.btn-primary::after {
		content:url("/wp-content/uploads/2020/04/logo-brand-right.png");
	position:absolute;
	top:-17px;
	left:-40px;
	transition:1s;

	
}

.btn-primary:hover {
	transition:1s;
	border-right: 0px solid;
		border-top: 1px solid black;
	border-bottom: 1px solid black;
	background-color:#fff;
	color:#fff;
	transition:1.25s;
	text-shadow: 0 0 5px #000, 0 0 10px #fff, 0 0 15px #fff, 0 0 10px #228DFF, 0 0 35px #228DFF, 0 0 15px #228DFF, 0 0 20px #228DFF, 0 0 25px #228DFF;
	
}

.btn-primary:hover:after {
	content:url("/wp-content/uploads/2020/04/logo-brand-right.png");
	left:calc(100% - 60px);
	transition:1s;
}

